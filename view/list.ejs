<% include head.ejs %>
<title>Members - Dating app</title>
<p>
</p>

<div class="container">
<h1>alle leden</h1>
<% for (var index = 0; index < data.length; index++) { %>
  <div class="row">
    <div class="col s12 m12">
      <div class="card medium">
        <div class="card-image">
          <% if (data[index].cover) { %>
          <img
            alt
            src="/upload/<%= data[index].cover %>"
          >
          <% } %>
          <span class="card-title"><%= data[index].name %></span>
        </div>
        <div class="card-content">
          <p><b>Beschrijving: </b><%= data[index].description %></p>
          <p><b>Leeftijd: </b><%= data[index].age %></p>
        </div>
        <div class="card-action">
          <a href="<%= data[index]._id %>">Bekijk profiel</a>
          <a href="">Like</a>
        </div>
      </div>
    </div>
  </div>
<% } %>
</div>
<% include tail.ejs %>
