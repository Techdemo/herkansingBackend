<% include head.ejs %>
<title>Members - Dating app</title>
<p>
<a href="/register">Registreer nieuwe gebruiker</a>
</p> 
<h1>alle leden</h1>
<% for (var index = 0; index < data.length; index++) { %>
  <div class="row">
    <div class="col s12 m12">
      <div class="card medium">
        <div class="card-image">
          <% if (data[index].cover) { %>
          <img
            alt
            src="/upload/<%= data[index].cover %>"
          >
          <% } %>
          <span class="card-title"><%= data[index].name %></span>
        </div>
        <div class="card-content">
          <p><%= data[index].description %></p>
          <p><%= data[index].age %></p>
        </div>
        <div class="card-action">
          <a href="<%= data[index]._id %>">Bekijk profiel</a>
          <a href="">Like</a>
        </div>
      </div>
    </div>
  </div>
<% } %>
